{
  "manifest_version": 3,
  "name": "Facebook Auto Expand Posts",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
